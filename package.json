{
  "name": "trykit",
  "version": "0.1.0",
  "description": "Simple magic-like utilities to ease your development experience",
  "private": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "homepage": "https://github.com/m10rten/trykit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/m10rten/trykit.git"
  },
  "bugs": {
    "url": "https://github.com/m10rten/trykit/issues"
  },
  "readme": "",
  "scripts": {
    "build": "tsup --format esm,cjs --dts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --passWithNoTests",
    "prettier:check": "prettier --check .",
    "format": "prettier --write .",
    "generate": "echo 'Not implemented yet.'",
    "precheck": "pnpm typecheck && pnpm lint",
    "verify": "pnpm build && pnpm test",
    "release": "changeset publish",
    "change": "changeset",
    "prepare": "husky"
  },
  "type": "commonjs",
  "keywords": [],
  "author": "m10rten",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "2.27.7",
    "@eslint/js": "9.9.1",
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.12",
    "@types/node": "22.5.1",
    "eslint": "9.9.1",
    "husky": "9.1.5",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsup": "8.2.4",
    "tsx": "4.19.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.3.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./safe-try": {
      "types": "./dist/safe-try.d.ts",
      "require": "./dist/safe-try.js",
      "import": "./dist/safe-try.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "safe-try": [
        "dist/safe-try.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
